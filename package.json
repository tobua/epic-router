{
  "name": "epic-react-router",
  "description": "MobX react router.",
  "version": "2.1.8",
  "repository": "github:tobua/epic-react-router",
  "homepage": "https://epic-react-router.vercel.app",
  "license": "MIT",
  "author": "Matthias Giger",
  "scripts": {
    "build": "padua build",
    "start": "padua watch",
    "test": "padua test"
  },
  "dependencies": {
    "history": "^5.0.1",
    "query-string": "^7.0.1",
    "url-join": "^4.0.1"
  },
  "peerDependencies": {
    "mobx": "^6.3.3",
    "mobx-react-lite": "^3.2.1",
    "react": "^17.0.2"
  },
  "main": "dist/index.js",
  "exports": {
    "default": "./dist/index.js"
  },
  "module": "dist/index.js",
  "sideEffects": false,
  "source": "index.tsx",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "index.tsx"
  ],
  "keywords": [
    "mobx",
    "react",
    "router",
    "web"
  ],
  "devDependencies": {
    "@types/history": "^4.7.9",
    "@types/react": "^17.0.27",
    "mobx": "^6.3.3",
    "mobx-react-lite": "^3.2.1",
    "padua": "^0.3.13",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-test-renderer": "^17.0.2"
  },
  "prettier": "padua/configuration/.prettierrc.json",
  "eslintConfig": {
    "extends": "./node_modules/padua/configuration/eslint.cjs",
    "rules": {
      "@typescript-eslint/lines-between-class-members": 0
    }
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsconfig": "./tsconfig.json"
      }
    },
    "testPathIgnorePatterns": [
      "/node_modules/",
      "dist"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  },
  "engines": {
    "node": ">= 14"
  }
}
